{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/example-app",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/example",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake: build",
      "miDebuggerPath": "/usr/bin/lldb",
      "logging": {
        "moduleLoad": true,
        "programOutput": true,
        "trace": true,
        "engineLogging": true,
        "exceptions": true
      },
      "debugServerPath": "",
      "filterStdout": false,
      "filterStderr": false,
      "serverStarted": "",
      "serverLaunchTimeout": 10000,
      "internalConsoleOptions": "openOnSessionStart",
      "showDisplayString": false
    }
  ]
}